{
  "coverageAnalysis": "off",
  "files": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "mutate": [
    "src/**/*.ts",
    "!src/tools/**/*"
  ],
  "mutator": "typescript",
  "packageManager": "npm",
  "reporters": [
    "clear-text",
    "progress"
  ],
  "testRunner": "jest",
  "thresholds": {
    "break": 100
  },
  "tsconfigFile": "tsconfig.json"
}
